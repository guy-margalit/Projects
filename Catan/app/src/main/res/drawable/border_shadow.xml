<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >
    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="@color/color_two"
                android:dashWidth="1dp"/>

            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="-1dp"
                android:top="-1dp" />

            <corners android:radius="3dp" />
        </shape>
    </item>
</layer-list>